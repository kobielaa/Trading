<div class="container">
  <div class="nav-logo">
    <h1>
      <a href="{{ url('<front>') }}">MK</a><span class="dot">.</span></h1>
        
  </div>

  {% import _self as menus %}

    {{ menus.menu_links(items, attributes, 0) }}

    {% macro menu_links(items, attributes, menu_level) %}
      {% import _self as menus %}
      {% if items %}
        {% if menu_level == 0 %}
          <ul class="main-navbar" {{ attributes }}>
        {% else %}
          <ul >
        {% endif %}
        {% for item in items %}
          <li{{ item.attributes }}>
            {{ link(item.title, item.url) }}
            {% if item.below %}
              {{ menus.menu_links(item.below, attributes, menu_level + 1) }}
            {% endif %}
          </li>
        {% endfor %}
        </ul>
      {% endif %}
    {% endmacro %}
</div>

